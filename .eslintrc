{
    "root": true,
    "env": { "node": true, "es6": true },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [ "sonarjs" ],
    "extends": [ "eslint:recommended", "plugin:sonarjs/recommended" ],
    "rules": {
      "sonarjs/no-duplicate-string": 0,
      "sonarjs/cognitive-complexity": ["error", 20]
    }
}